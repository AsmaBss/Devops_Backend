pipeline {
    agent any

    stages {
        stage('Git Cloning') {
            steps {
                git branch:'Asma',
                url: 'https://github.com/hazemAm28/TpAchat.git'
            }
        }
        stage('Mvn clean') {
            steps {
                sh 'mvn clean'
            }
        }
         stage('Mvn compile') {
            steps {
                sh 'mvn compile'
            }
        }
        
    }
}
