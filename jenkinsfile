pipeline {
    agent any

    stages {
        stage('Git Cloning') {
            steps {
                echo 'Getting project from Git';
                git branch: 'Asma',
                url: 'https://github.com/hazemAm28/TpAchat.git'
            }
        }
        stage('Clean Project') {
            steps {
                sh 'mvn clean'
            }
        }
        stage('Compile Project') {
            steps {
                sh 'mvn compile'
            }
        }
       
    }    
}
