pipeline {
    agent any

    stages {
        stage('Git') {
            steps {
                echo 'Getting project from Git';
                 git branch:'Fedi',
                 url: 'https://github.com/hazemAm28/TpAchat.git'
            }
        }
         stage('Mvn clean') {
            steps {
                sh 'mvn clean'
            }
        }
         stage('Mvn compile') {
            steps {
                sh 'mvn compile'
            }
        }
         stage('Mvn Junit') {
            steps {
                sh 'mvn test'   
            }
        }
         stage('Mvn sonar') {
            steps {
                sh 'mvn sonar:sonar -Dsonar.login=admin -Dsonar.password=sonar'
            }
        }
}
}
